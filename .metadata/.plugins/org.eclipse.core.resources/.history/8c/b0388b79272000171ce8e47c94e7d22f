package com.zuche;

public class zuche {
	int id;
	String name;
	int busload;
	int boatload;
	int rent;
	public zuche(int id,String name,int busload,int boatload,int rent){
		this.id = id;
		this.name = name;
		this.busload = busload;
		this.boatload = boatload;
		this.rent = rent;	
	}
	public void say(){
		System.out.println("序号为"+id+"车名是"+name+"载客数为"+busload+"人"+"载重量为"+boatload+"吨"+"日租金为"+rent+"元");	
	}
}
