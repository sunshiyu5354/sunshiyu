package Socket;

import java.net.*;
import java.util.*;

public class Server {
	public static void main(String args[]){
		try{
			ServerSocket ss = new ServerSocket(5354);
			System.out.println("端口创建成功");
			while(true){
				Scanner sc = new Scanner(System.in);
				Socket soc = ss.accept();//定义一个Socket对象
				
			}
		}catch(Exception e){
			e.printStackTrace();
		}
	}
}
